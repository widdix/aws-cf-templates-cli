{
  "name": "widdix",
  "version": "0.2.0",
  "description": "A CLI tool to manage Free Templates for AWS CloudFormation",
  "author": "Michael Wittig <michael@widdix.de>",
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-sdk/client-cloudformation": "3.41.0",
    "@aws-sdk/client-ec2": "3.41.0",
    "@aws-sdk/client-iam": "3.41.0",
    "@aws-sdk/client-sts": "3.41.0",
    "@aws-sdk/credential-providers": "3.41.0",
    "@aws-sdk/node-http-handler": "3.40.0",
    "docopt": "0.6.2",
    "md5": "2.3.0",
    "p-limit": "4.0.0",
    "proxy-agent": "5.0.0",
    "request": "2.88.2",
    "requestretry": "6.0.0",
    "semver": "7.3.5",
    "serialize-error": "9.0.0",
    "simple-node-logger": "21.8.12",
    "truncate-middle": "1.0.6"
  },
  "devDependencies": {
    "pkg": "5.4.1",
    "eslint": "8.2.0",
    "mocha": "9.1.3",
    "nock": "13.2.1",
    "stdio-mock": "1.1.0"
  },
  "scripts": {
    "test": "eslint . && AWS_ACCESS_KEY_ID=AKID AWS_SECRET_ACCESS_KEY=SECRET mocha -t 10000 test/*.js",
    "package": "pkg package.json --targets node16-linux-x64,node16-macos-x64,node16-win-x64"
  },
  "bin": "index.js",
  "pkg": {
    "assets": "cli.tx"
  },
  "type": "module"
}
